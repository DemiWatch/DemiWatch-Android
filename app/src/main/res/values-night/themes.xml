<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.DemiWatch" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryVariant">@color/white</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/blue</item>
        <item name="colorSecondaryVariant">@color/blue</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="textStyleMediumCustom">
        <item name="android:textColor">@color/black</item>
        <item name="fontFamily">@font/poppinsmedium</item>
    </style>

    <style name="textStyleSemiBoldCustom">
        <item name="android:textColor">@color/black</item>
        <item name="fontFamily">@font/poppinssemibold</item>
    </style>
</resources>